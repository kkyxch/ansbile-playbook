connect_timeout=30
network_timeout=60
base_path=/fdfs/
load_fdfs_parameters_from_tracker=true
storage_sync_file_max_delay = 86400
use_storage_id = false
storage_ids_filename = storage_ids.conf
tracker_server={{ tracker_server01 }}:22122
tracker_server={{ tracker_server02 }}:22122
storage_server_port=23000
group_name={{ group_name }}
url_have_group_name = true
store_path_count={{ store_size.stdout }}
{% for v in store_number.stdout_lines %}
store_path{{ v }}=/d{{ v }}
{% endfor %}
log_level=info
log_filename=/fdfs/logs/mod_fastdfs.log
response_mode=proxy
if_alias_prefix=
flv_support = true
flv_extension = flv
group_count = 0
#include http.conf
